{
    "displayName": "nestjs-client-keycloak",
    "name": "@fsdp0/nestjs-client-keycloak",
    "version": "0.0.207",
    "description": "NestJS based on keycloak-js adapter library",
    "readme": "./README.md",
    "license": "Apache-2.0",
    "type": "module",
    "author": {
        "name": "Ray Lee",
        "email": "fsdp0dev@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FSDP0/nestjs-client-keycloak.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": " https://registry.npmjs.org"
    },
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": "./dist/index.js",
        "./interfaces": "./dist/interfaces/index.js",
        "./providers": "./dist/providers/index.js",
        "./services": "./dist/services/index.js",
        "./constants": "./dist/constants/index.js",
        "./modules": "./dist/modules/index.js"
    },
    "scripts": {
        "compile": "tsc",
        "build:clean": "rimraf dist",
        "prebuild": "node --run build:clean",
        "build": "node --run compile",
        "prepublishOnly": "node --run build"
    },
    "keywords": [
        "NestJS",
        "Keycloak",
        "IAM"
    ],
    "dependencies": {
        "keycloak-js": "^26.2.0",
        "@nestjs/common": "^11.1.6",
        "@nestjs/core": "^11.1.6",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2"
    },
    "peerDependencies": {
        "keycloak-js": "^26.2.0"
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.2"
    }
}
